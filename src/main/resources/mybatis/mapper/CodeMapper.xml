<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="first.example.firstclass.dao.CodeDAO">
    <!-- Reject 코드 가져오기  -->
    <select id="findAllRejectCode" resultType="CodeDTO">
    	SELECT code, name
    	FROM TB_Code
    	WHERE code LIKE 'RJ%';
    </select>
</mapper>